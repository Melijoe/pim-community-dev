{
    "name": "pim/flexible-entity-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo Flexible Entity Bundle",
    "keywords": ["Akeneo"],
    "homepage": "https://github.com/akeneo/FlexibleEntityBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "stof/doctrine-extensions-bundle": "dev-master"
    },
    "require-dev": {
        "doctrine/dbal": ">=2.2,<2.4-dev",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "autoload": {
        "psr-0": { "Pim\\Bundle\\FlexibleEntityBundle": "" }
    },
    "target-dir": "Pim/Bundle/FlexibleEntityBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
